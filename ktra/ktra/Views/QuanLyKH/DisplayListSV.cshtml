@model IEnumerable<ktra.Models.SINHVIEN>
@{
    ViewBag.Title = "DisplayListSV";
    Layout = "~/Views/Shared/default.cshtml";
}

<h2>DisplayListSV</h2>
<table>
    <tr>
        <th><div>@Html.DisplayNameFor(model => model.sid)</div></th>
        <th><div>@Html.DisplayNameFor(model => model.sname)</div></th>
        <th><div>@Html.DisplayNameFor(model => model.sage)</div></th>
        <th><div>@Html.DisplayNameFor(model => model.diem)</div></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th><div>@Html.DisplayFor(modeItem => item.sid)</div></th>
            <th><div>@Html.DisplayFor(modeItem => item.sname)</div></th>
            <th><div>@Html.DisplayFor(modeItem => item.sage)</div></th>
            <th><div>@Html.DisplayFor(modeItem => item.diem)</div></th>
        </tr>
    }
</table>

